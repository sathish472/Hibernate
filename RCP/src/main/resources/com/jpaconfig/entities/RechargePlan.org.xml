<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" version="1.0">
    <entity class="com.jpaconfig.entity.RechargePlan">
        <table name="recharge_plan"/>
        <attributes>
            <id name="palnNo">
                <column name="paln_no"/>
            </id>
            <basic name="planName">
