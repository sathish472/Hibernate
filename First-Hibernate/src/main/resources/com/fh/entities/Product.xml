<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fh.entities.Product" table="product">
        <id name="productNo" column="PRODUCT_NO"/>
        <property name="productName" column="PRODUCT_NM"/>
        <property name="discription" column="DISCRIPTION"/>
        <property name="manufacturerName" column="MANUFACTURER_NAME"/>
        <property name="categoryName" column="CATEGORY_NAME"/>
        <property name="price" column="PRICE"/>
    </class>
</hibernate-mapping>